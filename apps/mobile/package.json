{
  "name": "@app-passeios-turisticos/mobile",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "clear-cache": "expo start --clear",
    "reset-cache": "npx expo start --clear && npx expo r -c",
    "reset-project": "node ./scripts/reset-project.js",
    "clean": "npx expo start --clear && rm -rf node_modules && npm install",
    "build:android": "eas build -p android --profile preview",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "prebuild": "npx expo start --clear"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
