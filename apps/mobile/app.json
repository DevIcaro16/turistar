{
  "expo": {
    "name": "Turistar",
    "slug": "@app-passeios-turisticos/Turistar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro",
      "output": "server"
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/notification_sound.wav",
            "./assets/notification_sound_other.wav"
          ],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.app_passeios_turisticos",
          "publishableKey": "pk_test_51RmZNjCmMj907B9mfpZByFn1ehbdjr7gjJpBna5JZtgTblB1WdNdzbixrs7kM7sNyXfONCb6oEFqDcyn2x5sqlv000aoAnINZN"
        }
      ]
    ]
  }
}